<form id="formMultimedia">
  <div class="form-group">
    <label for="fecha">Fecha</label>
    <input type="date" class="form-control" id="fecha" name="fecha" aria-describedby="emailHelp" placeholder="Ingresa una fecha">
    <small id="emailHelp" class="form-text text-muted">Ingresa una fecha.</small>
  </div>
  <div hidden class="form-group">
    <label for="fechaText">Compania</label>
    <input type="text" class="form-control" id="fechaText" name="fechaText" placeholder="Ingrese la Ciudad">
  </div>
  <div class="form-group">
    <label for="empresa">Empresa</label>
    <select class="form-control" id="empresa" name="empresa">
      <option disabled selected="selected">Selecciona una Empresa</option>
      <option>Aerolineas Argentinas</option>
      <option>Iberia</option>
      <option>Ryan Air</option>
      <option>American Airlines</option>
      <option>Latam</option>
      <option>Air Canada</option>
      <option>Air New Zealand</option>
    </select>
  </div>
  <div hidden class="form-group">
    <label for="empresaText">Compania</label>
    <input type="text" class="form-control" id="empresaText" name="empresaText" placeholder="Ingrese la Ciudad">
  </div>
  <div class="form-group">
    <label for="avion">Avion</label>
    <input type="text" class="form-control" id="avion" name="avion" placeholder="Ingrese un avion">
  </div>
  <div class="modal-footer">
    <div class="primary-button">
      <a id="botonBuscarMultimedia" href="#" data-toggle="modal" data-target="#modalMultimedia">Buscar Multimedia</a>
    </div>
    <div class="primary-button">
      <a href="#services">Volver al Menu</a>
    </div>
  </div>
</form>
